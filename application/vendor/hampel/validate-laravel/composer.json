{
    "name": "hampel/validate-laravel",
    "description": "Custom Laravel 4 Validator service provider composer package",
	"homepage" : "https://bitbucket.org/hampel/validate-laravel",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com",
		"homepage": "http://hampelgroup.com/",
		"role": "Developer"
	}],
	"support": {
		"email": "support@hampelgroup.com",
		"issues": "https://bitbucket.org/hampel/validate-laravel/issues",
		"source": "https://bitbucket.org/hampel/validate-laravel/src"
	},
	"keywords" : [
		"laravel",
		"validation"
	],
	"license" : [
		"MIT"
	],
    "require": {
		"hampel/validate": "~2.1",
		"illuminate/support": "~4.0",
		"illuminate/cache": "~4.0",
		"illuminate/config": "~4.0",
		"illuminate/container": "~4.0",
		"illuminate/log": "~4.0",
		"illuminate/translation": "~4.0",
		"illuminate/validation": ">=4.0.9,~4.0",
		"php": ">=5.3.0"
    },
	"require-dev" : {
		"mockery/mockery": "dev-master@dev",
		"phpunit/phpunit": "~4.0"
	},
    "autoload": {
         "psr-4": {
            "Hampel\\Validate\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
         "psr-4": {
            "Hampel\\Validate\\Laravel\\": "src/"
        }
    }
}